<!DOCTYPE html>
<html>
  <head>
    <title>Welp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <script type="text/javascript">
      window.searchIcon = "<%= image_url('white_searchicon.png')%>"
      window.backgroundPic = "<%= image_url('kbbq-background-pic-final.png')%>"
      window.restaurantIcon = "<%= image_url('categories/restaurant_icon.png')%>"
      window.shoppingIcon = "<%= image_url('categories/shopping_icon.png')%>"
      window.carIcon = "<%= image_url('categories/car_icon.png')%>"
      window.nightlifeIcon = "<%= image_url('categories/nightlife_icon.png')%>"
      window.tacos = "<%= image_url('popular_searches/tacos.jpg')%>"
      window.loungeBar = "<%= image_url('popular_searches/nightlife.jpg')%>"
      window.auto = "<%= image_url('popular_searches/auto.jpg')%>"
      window.shopping = "<%= image_url('popular_searches/shopping.jpg')%>"
      window.bottomImg = "<%= image_url('yelp_footer.png')%>"
      window.mainLogo = "<%= image_url('yelp_logo.png')%>"
    </script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
